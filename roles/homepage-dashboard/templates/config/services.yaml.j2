- Network:
    - FritzBox:
        icon: fritzbox.svg
        href: http://192.168.5.1/
        id: fritzBoxID
        ping: 192.168.5.1
        widget:
          type: fritzbox
          fields: ["uptime", "externalIPAddress", "down", "up"]
          url: http://192.168.5.1/
    - UCG Max Gateway:
        icon: unifi.svg
        href: https://192.168.10.1/
        id: unifiID
        ping: 192.168.10.1
        widget:
            type: unifi
            url: https://192.168.10.1
            #fields: ["lan_devices", "wlan_devices", "uptime"]
            username: {{ unifi_username }}
            password: {{ unifi_password }}
    - PiHole - Tyrion:
        icon: pi-hole.svg
        href: https://pihole.home-lorenzen.de/admin
        server: tyrion
        container: pihole
        widget:
          type: pihole
          url: "https://pihole.home-lorenzen.de"
          version: 6
          key: "{{ vault_pihole_password }}"
    - PiHole - Arya:
        icon: pi-hole.svg
        href: http://192.168.10.40:9090/admin/
        server: arya
        container: pihole
        ping: arya
        widget:
          type: pihole
          url: "http://192.168.10.40:9090"
          version: 6
          key: "{{ vault_pihole_password }}"

- Media:
