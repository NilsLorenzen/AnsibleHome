              - field: totalNumberOfHeads
                label: anzKöpfe
                format: number # optional - defaults to text
              - field: totalNumberOfShishas
                label: anzShishas
                format: number # optional - defaults to text
              - field: daysSinceLastSmoke
                label: seit Kopf
                format: number # optional - defaults to text
